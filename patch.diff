+              // io.socket.on('connect_failed', function () {
+              //  consolog('io.socket emitted `connect_failed`');
+              // });
+              // io.socket.on('reconnect_failed', function () {
+              //  consolog('io.socket emitted `reconnect_failed`');
+              // });
